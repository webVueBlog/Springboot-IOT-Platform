                <groupId>com.webVueBlog</groupId>
                <artifactId>webVueBlog-server</artifactId>
                <version>${webVueBlog.version}</version>
            </dependency>

            <!--工具插件管理类-->
            <dependency>
                <groupId>com.webVueBlog</groupId>
                <artifactId>webVueBlog-plugs</artifactId>
                <version>${webVueBlog.version}</version>
            </dependency>

            <!-- 业务模块-->
            <dependency>
                <groupId>com.webVueBlog</groupId>
