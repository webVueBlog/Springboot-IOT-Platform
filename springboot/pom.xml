            <dependency>
                <groupId>com.webVueBlog</groupId>
                <artifactId>boot-strap</artifactId>
                <version>${webVueBlog.version}</version>
            </dependency>
            <!--协议解析基础模块-->
            <dependency>
                <groupId>com.webVueBlog</groupId>
                <artifactId>webVueBlog-protocol-base</artifactId>
                <version>${webVueBlog.version}</version>
            </dependency>
            <!--网关启动模块-->
            <dependency>
                <groupId>com.webVueBlog</groupId>
                <artifactId>gateway-boot</artifactId>
