                <groupId>com.webVueBlog</groupId>
                <artifactId>webVueBlog-protocol-collect</artifactId>
                <version>${webVueBlog.version}</version>
            </dependency>
            <!--服务核心模块-->
            <dependency>
                <groupId>com.webVueBlog</groupId>
                <artifactId>iot-server-core</artifactId>
                <version>${webVueBlog.version}</version>
            </dependency>
